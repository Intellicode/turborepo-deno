{
  "$schema": "https://turborepo.com/schema.json",
   "daemon": false,
  "tasks": {
     "test": {
      "dependsOn": ["^test"]
    },
    "build": {
      "dependsOn": ["^build"]

    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
